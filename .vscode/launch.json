{
  "configurations": [
    {
      "name": "Run opened test (v3)",
      "type": "node",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "test:v3",
        "--",
        "--testTimeout",
        "100000",
        "--runTestsByPath",
        "${relativeFile}"
      ],
      "request": "launch",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Run opened test (v4)",
      "type": "node",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "test:v4",
        "--",
        "--testTimeout",
        "100000",
        "--runTestsByPath",
        "${relativeFile}"
      ],
      "request": "launch",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
